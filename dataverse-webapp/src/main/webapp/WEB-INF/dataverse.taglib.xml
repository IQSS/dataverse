<?xml version="1.0"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
        version="2.0">

    <namespace>http://dataverse.org/facelets</namespace>
    
    <tag>
        <tag-name>termsOfUseDialog</tag-name>
        <source>tags/editTermsOfUseDialog.xhtml</source>
    </tag>
    <tag>
        <description>Displays the given terms of use in user friendly form</description>
        <tag-name>displayTermsOfUse</tag-name>
        <source>tags/displayTermsOfUse.xhtml</source>
        <attribute>
            <description>Terms of use to display</description>
            <name>termsOfUse</name>
            <required>true</required>
            <type>edu.harvard.iq.dataverse.license.FileTermsOfUse</type>
        </attribute>
    </tag>

</facelet-taglib>
