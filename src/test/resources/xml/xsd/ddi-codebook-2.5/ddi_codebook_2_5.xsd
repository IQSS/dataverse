            <xs:attribute name="rectype" type="xs:string"/>
            <xs:attribute name="sdatrefs" type="xs:IDREFS"/>
            <xs:attribute name="methrefs" type="xs:IDREFS"/>
            <xs:attribute name="pubrefs" type="xs:IDREFS"/>
